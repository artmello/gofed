{"name": "gofed-scan",
"commands": [
	{"command": "apidiff", "help": "compare API of two commits of the same golang project", "script": "goapidiff/goapidiff.py"},
	{"command": "approx-deps", "help": "Approximate project dependencies", "script": "approx-deps/approx-deps.py"},
	{"command": "scan-deps", "help": "scan all golang packages for dependencies (e.g. cyclic)", "script": "scan-deps/scan-deps.py"},
	{"command": "scan-distro", "help": "scan distribution", "script": "scan-distro/scan-distro.py"},
	{"command": "scan-packages", "help": "scan packages", "script": "scan-packages/scan-packages.py"}
]}
